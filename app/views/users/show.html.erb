<%= provide(:title, @u.first_name) %>
<% flash.each do |message_type, message| %>
  <div class="alert alert-<%= message_type%>"><%= message%></div>
<% end %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
    <h1>
      <%= gravatar_for(@u) %>
      <%= @u.first_name  %>
    </h1>
  </section>
  </aside>
</div>
